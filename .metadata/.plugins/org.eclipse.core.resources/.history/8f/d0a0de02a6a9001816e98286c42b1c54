package com.example.model;

import javax.persistence.*;

import lombok.Getter;
import lombok.Setter;

@Entity
@Getter @Setter
public class School {
	@Id
	@Column(name = "SCHOOL_ID")
	private Long id;
	private String name;
	private String address;
	private String telnumber;
	
	public School(String name) {
		this.name = name;
	}
}
